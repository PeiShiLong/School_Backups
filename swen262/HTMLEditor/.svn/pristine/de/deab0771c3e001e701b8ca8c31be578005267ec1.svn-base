package main;

import javax.swing.ImageIcon;
import javax.swing.JMenuItem;

/**
 * MenuItemOption is a class that extends JMenuItem
 * for use in out code with menu items that change
 * state based on variables
 */
@SuppressWarnings("serial")
public class MenuItemOption extends JMenuItem {

    /**
     * Instantiation of a JMenuItem that will have changing icon
     * @param text
     * @param icon
     */
    public MenuItemOption(String text, ImageIcon icon){
        super(text, icon);
    }
    
}
